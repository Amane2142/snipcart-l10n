{
    "extends": "tslint-config-airbnb/tslint",  
    "linterOptions": {
        "exclude": [
            "node_modules/**"
        ]
    },
    "rules": {
        "ter-indent": [
            true,
            4, {
                "SwitchCase": 1
            }],
        "no-import-side-effect": true,
        "ordered-imports": [true, {
            "grouped-imports": true
        }],
        "import-name": [false]
    }
}